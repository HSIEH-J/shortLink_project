{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "no-empty": 0,
        "no-irregular-whitespace": 0,
        "quotes": ["error", "double"],
        "semi": [2, "always"],
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
}
